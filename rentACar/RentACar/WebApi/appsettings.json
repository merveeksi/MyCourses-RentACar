{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "RentACar": "Server=localhost;Port=5432;Database=RentACarDb;User Id=postgres;Password=mysecretpassword;"
  },
  "CacheSettings": {
    "SlidingExpiration": 2
  },
  "SerilogConfigurations": {
    "PostgreSqlConfiguration": {
      "ConnectionString": "Host=localhost;Port=5432;Database=RentACarDb;Username=postgres;Password=mysecretpassword;",
      "TableName": "Logs",
      "AutoCreateSqlTable": true
    },
    "FileLogConfiguration": {
      "FolderPath": "/logs/"
    }
  },
  "AllowedHosts": "*"
}